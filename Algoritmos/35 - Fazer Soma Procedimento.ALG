algoritmo "FAZER SOMA PROCEDIMENTO"

// Procedimento de Soma com parametro.

var
   X, Y: Inteiro

Procedimento Soma (A, B: Inteiro)
Inicio
      EscrevaL ("Recebi o valor ", A)
      EscrevaL ("Recebi o valor ", B)
      EscrevaL ("A soma entre os dois é ", A + B)
FimProcedimento

inicio
      X <- 5
      Y <- 3
      Soma (X, Y)
fimalgoritmo